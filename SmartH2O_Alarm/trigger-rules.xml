<?xml version="1.0" encoding="UTF-8"?>
<!--Para inserir um intervalo de valores utilize o seguinte metodo (valor1;valor2)-->
<sensors>
  <sensor type="PH">
    <conditions ative="true">
      <condition ative="true" operator="equal" value="6,1" />
      <condition ative="true" operator="greater_than" value="7" />
      <condition ative="true" operator="less_than" value="3" />
      <condition ative="true" operator="between" value="2;3" />
    </conditions>
  </sensor>
  <sensor type="CI2">
    <conditions ative="true">
      <condition ative="true" operator="equal" value="6,1" />
      <condition ative="true" operator="greater_than" value="7" />
      <condition ative="true" operator="less_than" value="3" />
      <condition ative="true" operator="between" value="2;3" />
    </conditions>
  </sensor>
  <sensor type="NH3">
    <conditions ative="true">
      <condition ative="true" operator="equal" value="6,1" />
      <condition ative="true" operator="greater_than" value="7" />
      <condition ative="true" operator="less_than" value="3" />
      <condition ative="true" operator="between" value="2;3" />
    </conditions>
  </sensor>
</sensors>